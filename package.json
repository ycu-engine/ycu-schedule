{
  "name": "ycu-schedule",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ycu-engine/ycu-schedule.git",
  "author": "YutaUra <yuuta3594@outlook.jp>",
  "license": "MIT",
  "scripts": {
    "pre-deploy": "yarn build:web",
    "pre-deploy:stage": "yarn build:web:stage",
    "pre-deploy:prod": "yarn build:web:prod",
    "deploy": "yarn pre-deploy && sls deploy",
    "deploy:stage": "yarn pre-deploy:stage && sls deploy --stage stage",
    "deploy:stage--first": "cross-env FIRST_DEPLOY=true sls deploy --stage stage",
    "deploy:prod": "cross-env SERVERLESS_ENV=prod yarn pre-deploy:prod && cross-env SERVERLESS_ENV=prod yarn sls deploy --stage prod",
    "deploy:prod--first": "cross-env FIRST_DEPLOY=true SERVERLESS_ENV=prod sls deploy --stage prod",
    "deploy--no-predeploy": "sls deploy",
    "deploy--first": "cross-env FIRST_DEPLOY=true yarn sls deploy",
    "dev": "cross-env AWS_REGION=\"us-east-1\" run-p dev:*",
    "dev:lambda": "sls offline",
    "dev:web": "yarn workspace web develop",
    "build:web": "yarn workspace web build",
    "build:web:stage": "yarn workspace web build",
    "build:web:prod": "yarn workspace web build",
    "build:web--no-prebuild": "yarn workspace web build",
    "serve:web": "yarn workspace web serve",
    "type-check": "run-p type-check:*",
    "type-check:root": "tsc --noEmit",
    "type-check:web": "yarn workspace web type-check",
    "format": "run-p format:*",
    "format:api": "prettier --parser typescript --write ./**/*.ts",
    "format-check": "run-p format-check:*",
    "format-check:api": "prettier --parser typescript -c ./**/*.ts",
    "lint": "run-p lint:*",
    "lint:root": "eslint . --ignore-pattern \"web/**/*\" --ext .ts,.js",
    "lint:web": "yarn workspace web lint",
    "postinstall": "husky install && typesync || :"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "web"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "web/**/*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "serverless": "^2.28.7",
    "serverless-cloudfront-invalidate": "^1.8.0",
    "serverless-cloudfront-lambdaedge-plugin": "^1.0.4",
    "serverless-domain-manager": "^5.1.0",
    "serverless-s3-sync": "^1.16.1",
    "serverless-webpack": "^5.3.5",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.28.7",
    "@types/aws-lambda": "^8.10.72",
    "@types/eslint": "^7.2.6",
    "@types/node": "^14.14.31",
    "@types/prettier": "^2.2.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/serverless": "^1.78.20",
    "@types/source-map-support": "^0.5.3",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.2.3",
    "typesync": "^0.8.0",
    "webpack": "^5.24.3",
    "webpack-node-externals": "^2.5.2"
  }
}
