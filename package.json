{
  "name": "ycu-schedule",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ycu-engine/ycu-schedule.git",
  "author": "YutaUra <yuuta3594@outlook.jp>",
  "license": "MIT",
  "scripts": {
    "pre-deploy": "yarn build:web",
    "deploy": "yarn pre-deploy && sls deploy",
    "deploy--no-predeploy": "sls deploy",
    "dev": "cross-env AWS_REGION=\"us-east-1\" run-p dev:*",
    "dev:dynamodb": "sls dynamodb start --migrate",
    "dev:lambda": "sls offline",
    "dev:web": "yarn workspace web develop",
    "pre-build:web": "sls info",
    "build:web": "yarn pre-build:web && yarn workspace web build",
    "build:web--no-prebuild": "yarn workspace web build",
    "serve:web": "yarn workspace web serve",
    "type-check": "tsc --noEmit",
    "format": "run-p format:*",
    "format:api": "prettier --write ./**/*.ts",
    "format-check": "run-p format-check:*",
    "format-check:api": "prettier -c ./**/*.ts",
    "lint": "eslint . --ext .ts,.js",
    "postinstall": "husky install && typesync || :"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "web"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@middy/core": "^1.5.2",
    "@middy/http-json-body-parser": "^1.5.2",
    "@ycu-engine/serverless-stack-output": "^1.0.1",
    "json-schema-to-ts": "^1.6.0",
    "node-fetch": "^2.6.1",
    "serverless": "^2.28.7",
    "serverless-dynamodb-autoscaling": "^0.6.2",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^6.8.0",
    "serverless-s3-sync": "^1.16.1",
    "serverless-webpack": "^5.3.5",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.28.7",
    "@types/aws-lambda": "^8.10.72",
    "@types/eslint": "^7.2.6",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.8",
    "@types/prettier": "^2.2.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/serverless": "^1.78.20",
    "@types/source-map-support": "^0.5.3",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.2.3",
    "typesync": "^0.8.0",
    "webpack": "^5.24.3",
    "webpack-node-externals": "^2.5.2"
  }
}
